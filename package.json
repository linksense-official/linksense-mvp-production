{
  "name": "linksense-mvp",
  "version": "1.0.0",
  "private": true,
  "description": "LinkSense MVP - リモートワーク特化型チーム健全性分析SaaS",
  "homepage": "https://linksense-mvp.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/linksense-mvp.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/linksense-mvp/issues"
  },
  "author": "LinkSense Team",
  "license": "MIT",
  "keywords": [
    "saas",
    "team-management",
    "oauth",
    "authentication",
    "nextauth",
    "social-login",
    "security",
    "2fa",
    "remote-work",
    "analytics",
    "nextjs",
    "typescript",
    "stripe",
    "prisma",
    "vercel"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "echo 'ESLint check skipped for production build'",
    "lint:check": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:social-login": "jest src/tests/auth/social-login.test.tsx",
    "test:auth": "jest src/tests/auth/",
    "test:security": "jest src/tests/security/",
    "test:integration": "jest src/tests/integration/",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:init": "prisma generate && prisma db push --accept-data-loss",
    "build:analyze": "set ANALYZE=true && npm run build",
    "build:production": "npm run clean && npm run db:generate && prisma db push --accept-data-loss && next build",
    "build:staging": "set NODE_ENV=staging && npm run build",
    "build:safe": "prisma generate && next build",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:staging": "vercel --target staging",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:check": "npm run typecheck && npm run lint:check && npm run test:ci",
    "health:check": "node scripts/health-check.js",
    "health:db": "node scripts/db-health.js",
    "health:oauth": "node scripts/oauth-health.js",
    "logs:vercel": "vercel logs",
    "logs:production": "vercel logs --prod",
    "clean": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "clean:all": "if exist .next rmdir /s /q .next && if exist node_modules rmdir /s /q node_modules && npm install",
    "clean:force": "node scripts/clean-windows.js",
    "postinstall": "prisma generate",
    "prebuild": "npm run db:generate",
    "prestart": "npm run db:generate",
    "env:validate": "node scripts/validate-env.js",
    "env:example": "node scripts/generate-env-example.js",
    "env:backup": "copy .env.local .env.local.backup.%date:~10,4%%date:~4,2%%date:~7,2%_%time:~0,2%%time:~3,2%%time:~6,2% >nul 2>&1",
    "setup:production": "npm run env:validate && npm run db:migrate:deploy && npm run build:production",
    "setup:development": "npm install && npm run db:push && npm run db:seed",
    "maintenance:start": "node scripts/maintenance-mode.js --enable",
    "maintenance:stop": "node scripts/maintenance-mode.js --disable",
    "metrics:collect": "node scripts/collect-metrics.js",
    "metrics:report": "node scripts/generate-metrics-report.js",
    "backup:db": "node scripts/backup-database.js",
    "backup:env": "node scripts/backup-env.js",
    "ngrok": "node scripts/start-ngrok.js",
    "ngrok:simple": "node scripts/simple-ngrok.js",
    "ngrok:latest": "node scripts/ngrok-latest.js",
    "ngrok:manual": "ngrok http 3000",
    "ngrok:kill": "taskkill /F /IM ngrok.exe 2>nul || echo ngrok not running",
    "ngrok:update": "npm install -g ngrok@latest",
    "ngrok:version": "ngrok version",
    "ngrok:localtunnel": "npx localtunnel --port 3000",
    "ngrok:cloudflare": "npx wrangler tunnel --url http://localhost:3000",
    "dev:ngrok": "npm run ngrok:latest",
    "dev:tunnel": "npm run ngrok:localtunnel",
    "setup:ngrok": "node scripts/dev-setup.js",
    "quick:ngrok": "node scripts/quick-ngrok.js",
    "clean:ngrok": "npm run ngrok:kill && timeout /t 2 /nobreak >nul && npm run ngrok:latest",
    "check:ngrok": "node scripts/check-ngrok.js",
    "tunnel:status": "curl -s http://localhost:4040/api/tunnels 2>nul || echo ngrok not running",
    "build:android": "npx cap add android && npx cap copy && npx cap open android",
    "build:ios": "npx cap add ios && npx cap copy && npx cap open ios",
    "build:pwa": "next build"
  },
  "dependencies": {
  "@heroicons/react": "^2.2.0",
  "@next-auth/prisma-adapter": "^1.0.7",
  "@prisma/client": "^6.8.2",
  "@radix-ui/react-alert-dialog": "^1.1.14",
  "autoprefixer": "^10.4.16",
  "bcryptjs": "^3.0.2",
  "critters": "^0.0.23",
  "crypto-js": "^4.2.0",
  "lucide-react": "^0.511.0",
  "next": "^15.3.2",
  "next-auth": "^4.24.11",
  "openai": "^5.1.0",
  "otplib": "^12.0.1",
  "postcss": "^8.4.31",
  "prisma": "^6.8.2",
  "qrcode": "^1.5.4",
  "react": "^18.2.0",
  "react-dom": "^18.2.0",
  "resend": "^4.5.1",
  "stripe": "^18.1.1",
  "tailwindcss": "^3.4.0",
  "zod": "^3.22.4"
},
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.51",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-security": "^2.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsonwebtoken": "^9.0.2",
    "localtunnel": "^1.8.3",
    "next-pwa": "^5.6.0",
    "ngrok": "^5.0.0-beta.2",
    "open": "^8.4.0",
    "prettier": "^3.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.3.4",
    "typescript": "^5.0.0",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": false
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ],
    "plugins": [
      "security"
    ],
    "rules": {
      "security/detect-object-injection": "warn",
      "security/detect-non-literal-regexp": "warn",
      "security/detect-unsafe-regex": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/your-username"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.6.7"
  }
}
